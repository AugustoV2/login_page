generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model Info {
  id String @id @default(auto()) @map("_id") @db.ObjectId 
  firstname String
  lastname String
  company String
  phone Int
  weburl String
  unique String
  email String
  pass String 
  confpass String
}

model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  email String
}



